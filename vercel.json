{
    "builds": [{
        "src": "boltshift/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.11"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "boltshift/wsgi.py"
        }
    ]
}